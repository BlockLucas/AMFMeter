  /op/GetItemsCount:
    description: retrieves samples and products counts
    get:
      queryParameters:
        wo:
          description: Work Order Number
          required: true
          type: number
        customerId:
          description: customer owner of the work order, required for designer access tokens
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
             example: |
                {
                  "status": true,
                  "wo": 20501146,
                  "counts": {
                    "samples": "1",
                    "products": "0",
                    "total": "1"
                  }
                }
  /op/MoveItem:
    description: Moves an item from 1 work order to another
    post:
      queryParameters:
        wo:
          description: source work order
          required: true
          type: number
        item:
          description: item to move
          required: true
          type: number
        toWo:
          description: destination work order, if not given, the item will be moved to the current cart if no current cart, a new cart is created (if the access token is tied to a consumer). if the access token is tied to a designer instead, a new work order will be created and will be part of the response, to be able to add items to it if desired
          type: number
          required: false
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer, this is used by the system to make sure that the wo is owned by this customerId. customerId is not used if the access token is tied to a consumer
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                  {
                    "status" : true,
                    "message" : "Item 3 is moved from Work Order# 24112165 to Work Order# 24112169"
                  }

  /op/SaveItemValues:
    description: Save Work Order Item values
    post:
      queryParameters:
        wo:
          description: Work Order Number, if not given, the active cart is used, if no active cart, an error will be returned
          required: false
          type: number
        item:
          description: Work Order Item Number
          required: true
          type: number
        data:
          description: comma separated values of field names and values, for example "foo1:bar1,foo2:bar2" will save two fields foo1 and foo2 with the values bar1 and bar2 respectively
          required: true
          type: string
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer, this is used by the system to make sure that the wo is owned by this customerId. customerId is not used if the access token is tied to a consumer
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                  {
                    "status":true,
                    "message":"Item values saved"
                  }
  /op/UpdateItem:
    description: Update Work Order Item
    post:
      queryParameters:
        wo:
          description: Work Order Number, for comsumers and if wo not specified, the active cart is used, if no active cart, an error will be returned
          required: false
          type: number
        item:
          description: Work Order Item Number
          required: true
          type: number
        data:
          description: comma separated values of field names and values, for example "Model:bar1,Product:bar2" will update two fields Model and Product and set the following values bar1 and bar2 respectively
          required: true
          type: string
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer, this is used by the system to make sure that the wo is owned by this customerId. customerId is not used if the access token is tied to a consumer
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                  {
                    "status":true,
                    "message":"Item 3 updated"
                  }
  /op/SaveItem:
    description: Add Items to a work order, if Samples is present in the parameters, this will add samples, if Components is in params, this will add Components, if none of Samples and Components is present, this will add a Blind
    post:
      queryParameters:
        wo:
          description: Work Order number, if not specified, a new work order will be created
          required: false
          type: number
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer,  consumers don't have to specify customerId
          required: false
          type: string
        appointmentId:
          description: required when adding a new work order (when wo is not specified and access token is tied to a designer),
          type: number
          required: false
        Samples:
          description: if present as a parameter, this will add samples to the work order
          type: boolean
          required: false
        color1:
          description: swatch to add to the work order, use color2, color3, ...color4 to add multiples samples at the same time. this will probably be improved in the future to pass samples as a comma separated value and allow passing other values then colors
          required: false
        Components:
          description: if present, this assumes that components will be added to the work order
          type: boolean
          required: false
        stock_1:
          description: Stock to add, use stock_2,stock_3,....stock_4 to add multiples components at once
          required: false
        qty_1:
          description: qty to order, use qty_1, qty_2, .....qty_3 to specify multpiles quantities, 1 per stock
          required: false
        group_1:
          description: product group ID
          required: false
        product_1:
          description: product ID
          required: false
        Answers1:
          description: prompt answers, from Answers1 to Answers400, applicable for all cases
          required: false
        Colors1:
          description: Colors, from Colors1 to Colors400, applicable for all cases
          required: false
        Product:
          description: Only required when adding a blind
          required: false
        Model:
          description: Only required when adding a blind
          required: false
        Color:
          description: Only required when adding a blind
          required: false
        sidemark:
          description: work order sidemark, only used when creating a new work order
          type: string
          required: false

      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "stauts": true,
                  "items": [
                    {
                      "Wo": "20500827",
                      "Item": "1",
                      "GGroup": "99999",
                      "Product": "9000",
                      "Model": "",
                      "Stock": "14241",
                      "Color": "",
                      "Qty": "1.000",
                      "QtyPacked": "0.000",
                      "QtyShipped": "0.000",
                      "ReliefPurchase": "R",
                      "Warehouse": "1",
                      "ManualWhs": "",
                      "ShipVia": "",
