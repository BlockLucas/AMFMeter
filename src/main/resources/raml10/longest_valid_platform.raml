#%RAML 1.0
title: ePIC REST API
version: v1
baseUri: https://e-pic.picbusiness.com/16903TEST/php/Rest/index.php/api/
securitySchemes:
    oauth_2_0:
      description: |
          ePIC API supports OAuth 2.0 for authenticating all API requests.
      type: OAuth 2.0
      describedBy:
          headers:
              Authorization:
                  description: |
                     Used to send a valid OAuth 2 access token. Do not use
                     with the "access_token" query string parameter.
                  type: string
          queryParameters:
              access_token:
                  description: |
                     Used to send a valid OAuth 2 access token. Do not use together with the "Authorization" header
                  type: string
          responses:
              401:
                  description: |
                      Bad or expired token. This can happen if the access token is expired. To fix, you should re-
                      authenticate the user.
              403:
                  description: |
                      Bad OAuth request (wrong consumer key, bad nonce, expired
                      timestamp...). Unfortunately, re-authenticating the user won't help here.
      settings:
        authorizationUri: https://e-pic.picbusiness.com/16903TEST/php/Rest/index.php/oauth2/authorize
        accessTokenUri: https://e-pic.picbusiness.com/16903TEST/php/Rest/index.php/oauth2/token
        authorizationGrants: [ 'authorization_code', 'password','client_credentials' ]
securedBy: oauth_2_0
types:
  Appointment:
    type: object
    properties:
      Appointment: number
      Customer: string
      Status: number
      PriorAppointment: number
      ProductOrder: number
      InstallerOrder: number
      Roles: Roles
  DateObject:
    type: object
    properties:
      date: datetime
      timezone_type: number
      timezone: number
  Role:
    type: object
    properties:
      StartTime: DateObject
      IsPrimary: boolean
      TimeRequired: number
      Name: string
      Phone: string
      Email: string
      Id: number
      RessourceType: string
  Roles:
    type: object
    properties:
      DESIGNER:
        type: Role
        required: true
      INSTALLER:
        type: Role
/salespersons:
  description: load information about the current designer
/loginids:
  description: load user tied to the current designer
/orders:
  /op/LoadCustomerOrders:
    description: load work order owned by a customer and added by the current designer
    get:
      queryParameters:
        customerId:
          description: customer ID
          required: true
  /op/SaveOrder:
    description: Save a work order
    post:
      queryParameters:
        customerId:
          description: customer, owner of the work order, required if the access token is tied to a designer
          required: false
        wo:
          description: work order to save, if not specified, a new work order will be created
          required: false
        data:
          description: comma separated values of fields and values to save, example "ShName:foo,ShPhone:bar,...etc"
          required: true
        appointmentId:
          description: Appointment Id, must be supplied to create a new work order, appointment id must also belong to customer id for the request to be successful
          type: number
          required: false
        sidemark:
          description: work order sidemark, only used when creating a new work order
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "status": true,
                  "message": "Work Order# 20500829 saved"
                }
/orderitems:
  /op/DeleteItem:
    description: Delete order items, this end point is only accessible by consumers and designers
    post:
      queryParameters:
        wo:
          description: Work Order, if not set, this endpoint will use the current cart number if the access token is tied to a consumer and if there is an active cart, this is always required for designers
          type: number
          required: false
        item:
          description: which item to delete, set item to 0 to empty the work order (cart), all_products to delete all products, all_samples to delete all samples, set to 1 to delete item 1, ...etc
          type: string
          required: true
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer, this is used by the system to make sure that the wo is owned by this customerId. customerId is not used if the access token is tied to a consumer
          required: false
          type: string
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "status":true,
                  "message": "The following items have been deleted (2,5,9)"
                }
  /op/LoadOrderItem:
    description: Load an order item
    get:
      queryParameters:
        default:
          description: whether to load order item defaults - if present, wo and item are ignored and the endpoint returns order item defaults
          required: false
          type: boolean
        wo:
          description: work order number, if not present, the endpoints will use the Wo in the consumer record (current Cart number), if set
          required: false
          type: number
        item:
          description: which item to load, required unless default is used, in which case it is ignored
          required: false
          type: number
        customerId:
          description: the customer owner of the work order, this is required when the access token is tied to a designer, this is used by the system to make sure that the wo is owned by this customerId. customerId is not used if the access token is tied to a consumer
